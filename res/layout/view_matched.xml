        <com.tinder.views.CustomTextView app:textSize="@dimen/matched_text_size" app:textColor="@c1470c/white" app:gravity="1" app:id="@id/matched_text" app:paddingBottom="@dimen/padding_large" app:layout_width="-1" app:layout_height="-2" app:text="You and Jennifer have liked each other." app:layout_above="@id/linear_match_one" app:font="0" />
