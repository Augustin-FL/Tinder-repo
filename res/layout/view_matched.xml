        <RelativeLayout app:id="@id/linear_match_two" app:background="@drawable/rounded_white_bg" app:layout_width="@dimen/matched_avatar_frame_length" app:layout_height="@dimen/matched_avatar_frame_length" app:layout_marginLeft="@dimen/margin_xsmall" app:layout_toRightOf="@id/center_div" app:layout_above="@id/center_div">
