<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/transparent" />
            <stroke android:width="1dp" android:color="@color/selected_gray_button_color" />
            <corners android:radius="@dimen/discover_off_button_radius_corners" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/transparent" />
            <stroke android:width="1dp" android:color="@color/another_freaking_grey" />
            <corners android:radius="@dimen/discover_off_button_radius_corners" />
        </shape>
    </item>
</selector>
